<%= form_with url: merchant_items_path(@merchant.id), method: :post, local: true do |form| %>
<%= form.label :name %>
<%= form.text_field :name %>
<%= form.label :description %>
<%= form.text_field :description %>
<%= form.label :unit_price %>
<%= form.text_field :unit_price %>
<%= form.hidden_field :enabled, value: false %>
<%= form.submit 'Submit' %>
<% end %>
